<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KEL2PTB"
        tools:targetApi="31">
        <activity
            android:name=".NotificationActivity"
            android:exported="false" />
        <!--
             TODO: Before you run your application, you need a Google Maps API key.

             To get one, follow the directions here:

                https://developers.google.com/maps/documentation/android-sdk/get-api-key

             Once you have your API key (it starts with "AIza"), define a new property in your
             project's local.properties file (e.g. MAPS_API_KEY=Aiza...), and replace the
             "YOUR_API_KEY" string in this file with "${MAPS_API_KEY}".
        -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="YOUR_API_KEY" />

        <activity
            android:name=".MapsActivity"
            android:exported="false"
            android:label="@string/title_activity_maps" />
        <activity
            android:name=".EditProfileActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".UpdatePraktikanActivity"
            android:exported="false" />
        <activity
            android:name=".DetailPraktikanActivity"
            android:exported="false" />
        <activity
            android:name=".AddPraktikanActivity"
            android:exported="false" />
        <activity
            android:name=".UpdateAsistenActivity"
            android:exported="false" />
        <activity
            android:name=".AddAsistenActivity"
            android:exported="false" />
        <activity
            android:name=".DetailAsistenActivity"
            android:exported="false" />
        <activity
            android:name=".DataClassAsisten"
            android:exported="false" />
        <activity
            android:name=".DetailTaskActivity"
            android:exported="false" />
        <activity
            android:name=".UpdateTaskActivity"
            android:exported="false" />
        <activity
            android:name=".UploadTaskActivity"
            android:exported="false" />
        <activity
            android:name=".ListAsistenActivity"
            android:exported="false" />
        <activity
            android:name=".ListPraktikanActivity"
            android:exported="false" />
        <activity
            android:name=".ListTaskActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />


        <service
            android:name=".MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>


    </application>



</manifest>